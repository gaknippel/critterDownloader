{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:window:allow-create",
    "core:window:allow-close",
    "core:default",
    "dialog:allow-open",
    "store:allow-get",
    "store:allow-set",
    "store:allow-load",
    "store:allow-save"
  ]
}
